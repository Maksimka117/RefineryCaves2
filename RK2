                    Duration = 3,
                    Image = 0,
                })
                return
            end
            selectedTrees[treeName] = Value
            if treeEspEnabled then
                updateTreeESP()
            end
        end,
    })
end

ESPTab:CreateToggle({
   Name = "üå≥ Enable Tree ESP",
   CurrentValue = false,
   Flag = "TreeESPToggle",
   Callback = function(Value)
      if not isUnlocked then
         Rayfield:Notify({
            Title = "üîí Locked",
