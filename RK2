-- Teleports Tab
local TeleportsTab = Window:CreateTab("üìç Teleports", 0)

local TeleportsSection = TeleportsTab:CreateSection("üìç Teleport Locations")

local selectedLocation = ""

local function teleportToLocation(locationName)
    if not isUnlocked then
        Rayfield:Notify({
            Title = "üîí Locked",
            Content = "Please verify key first!",
            Duration = 3,
            Image = 0,
        })
        return
    end
    
    local structuresFolder = workspace:FindFirstChild("Map")
    if structuresFolder then
        structuresFolder = structuresFolder:FindFirstChild("Structures")
        if structuresFolder then
            local targetStructure = structuresFolder:FindFirstChild(locationName)
            if targetStructure then
                local character = LocalPlayer.Character
                if character then
                    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                    if humanoidRootPart then
                        -- Find a part in the structure to teleport to
                        local targetPart = targetStructure:FindFirstChildOfClass("Part") or targetStructure:FindFirstChildOfClass("MeshPart")
                        if targetPart then
                            humanoidRootPart.CFrame = CFrame.new(targetPart.Position + Vector3.new(0, 10, 0))
                            Rayfield:Notify({
                                Title = "üìç Teleport",
                                Content = "Teleported to " .. locationName,
                                Duration = 3,
                                Image = 0,
                            })
                        else
                            Rayfield:Notify({
                                Title = "‚ùå Error",
                                Content = "No valid part found in " .. locationName,
                                Duration = 3,
                                Image = 0,
                            })
                        end
                    end
                end
            else
                Rayfield:Notify({
                    Title = "‚ùå Error",
                    Content = "Location " .. locationName .. " not found",
                    Duration = 3,
                    Image = 0,
                })
            end
        else
            Rayfield:Notify({
                Title = "‚ùå Error",
                Content = "Structures folder not found",
                Duration = 3,
                Image = 0,
            })
        end
    else
        Rayfield:Notify({
            Title = "‚ùå Error",
            Content = "Map folder not found",
            Duration = 3,
            Image = 0,
        })
    end
end

local LocationDropdown = TeleportsTab:CreateDropdown({
   Name = "Select Location",
   Options = teleportLocations,
   CurrentOption = "",
   Flag = "LocationDropdown",
   Callback = function(Option)
      selectedLocation = Option
   end,
})

TeleportsTab:CreateButton({
   Name = "üìç Teleport",
   Callback = function()
      if selectedLocation ~= "" then
         teleportToLocation(selectedLocation)
      else
         Rayfield:Notify({
            Title = "‚ùå Error",
            Content = "Please select a location first",
            Duration = 3,
            Image = 0,
         })
      end
   end,
})

