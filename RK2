-- ESP Tab
local ESPTab = Window:CreateTab("üéØ ESP", 0)

-- Player ESP
local PlayerESPSection = ESPTab:CreateSection("üë§ Player ESP")

local function togglePlayerESP()
    playerEspEnabled = not playerEspEnabled
    
    if playerEspEnabled then
        Rayfield:Notify({
            Title = "üéØ Player ESP",
            Content = "Player ESP enabled",
            Duration = 3,
            Image = 0,
        })
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                local function addESP(character)
                    if character and not espHighlights[player] then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "PlayerESP_Highlight"
                        highlight.Adornee = character
                        highlight.FillColor = Color3.fromRGB(255, 0, 0)
                        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
                        highlight.FillTransparency = 0.5
                        highlight.OutlineTransparency = 0
                        highlight.Parent = character
                        
                        espHighlights[player] = highlight
                    end
                end
                
                if player.Character then
                    addESP(player.Character)
                end
                
                player.CharacterAdded:Connect(function(character)
                    addESP(character)
                end)
            end
        end
    else
        Rayfield:Notify({
            Title = "üéØ Player ESP",
            Content = "Player ESP disabled",
            Duration = 3,
            Image = 0,
        })
        for player, highlight in pairs(espHighlights) do
            if highlight and highlight.Name == "PlayerESP_Highlight" then
                highlight:Destroy()
                espHighlights[player] = nil
            end
        end
    end
end

ESPTab:CreateToggle({
   Name = "üë§ Player ESP",
   CurrentValue = false,
   Flag = "PlayerESPToggle",
   Callback = function(Value)
      if not isUnlocked then
         Rayfield:Notify({
            Title = "üîí Locked",
            Content = "Please verify key first!",
            Duration = 3,
            Image = 0,
         })
         return
      end
      if Value then
         togglePlayerESP()
      else
         if playerEspEnabled then
            togglePlayerESP()
         end
      end
   end,
})

-- Ore ESP
local OreESPSection = ESPTab:CreateSection("‚õèÔ∏è Ore ESP")

local oreHighlights = {}

local function updateOreESP()
    -- Clear existing ore highlights
    for oreName, highlight in pairs(oreHighlights) do
        if highlight then
            highlight:Destroy()
        end
    end
    oreHighlights = {}
    
    if oreEspEnabled then
        local oresFolder = workspace:FindFirstChild("WorldSpawn")
        if oresFolder then
            oresFolder = oresFolder:FindFirstChild("Ores")
            if oresFolder then
                for _, ore in pairs(oresFolder:GetDescendants()) do
                    if ore:IsA("Part") or ore:IsA("MeshPart") then
                        local oreName = ore.Name
                        if selectedOres[oreName] then
                            local highlight = Instance.new("Highlight")
                            highlight.Name = "OreESP_" .. oreName
                            highlight.Adornee = ore
                            highlight.FillColor = Color3.fromRGB(0, 255, 0)
                            highlight.OutlineColor = Color3.fromRGB(255, 255, 0)
                            highlight.FillTransparency = 0.3
                            highlight.OutlineTransparency = 0
                            highlight.Parent = ore
                            
                            oreHighlights[oreName] = highlight
                        end
                    end
                end
            end
        end
    end
end

local function toggleOreESP()
    oreEspEnabled = not oreEspEnabled
    
    if oreEspEnabled then
        Rayfield:Notify({
            Title = "‚õèÔ∏è Ore ESP",
            Content = "Ore ESP enabled",
            Duration = 3,
            Image = 0,
        })
        updateOreESP()
        
        -- Listen for new ores
        local oresFolder = workspace:FindFirstChild("WorldSpawn")
        if oresFolder then
            oresFolder = oresFolder:FindFirstChild("Ores")
            if oresFolder then
                oresFolder.DescendantAdded:Connect(function(descendant)
                    if oreEspEnabled and (descendant:IsA("Part") or descendant:IsA("MeshPart")) then
                        if selectedOres[descendant.Name] then
                            wait(0.1) -- Wait for ore to load
                            updateOreESP()
                        end
                    end
                end)
            end
        end
    else
        Rayfield:Notify({
            Title = "‚õèÔ∏è Ore ESP",
            Content = "Ore ESP disabled",
            Duration = 3,
            Image = 0,
        })
        for oreName, highlight in pairs(oreHighlights) do
            if highlight then
                highlight:Destroy()
            end
        end
        oreHighlights = {}
    end
end

-- Create ore selection
for _, oreName in pairs(allOres) do
    ESPTab:CreateToggle({
        Name = oreName,
        CurrentValue = false,
        Flag = "OreSelect_" .. oreName,
        Callback = function(Value)
            if not isUnlocked then
                Rayfield:Notify({
                    Title = "üîí Locked",
                    Content = "Please verify key first!",
                    Duration = 3,
                    Image = 0,
                })
                return
            end
            selectedOres[oreName] = Value
            if oreEspEnabled then
                updateOreESP()
            end
        end,
    })
end

ESPTab:CreateToggle({
   Name = "‚õèÔ∏è Enable Ore ESP",
   CurrentValue = false,
   Flag = "OreESPToggle",
   Callback = function(Value)
      if not isUnlocked then
         Rayfield:Notify({
            Title = "üîí Locked",
            Content = "Please verify key first!",
            Duration = 3,
            Image = 0,
         })
         return
      end
      if Value then
         toggleOreESP()
      else
         if oreEspEnabled then
            toggleOreESP()
         end
      end
   end,
})

-- Tree ESP
local TreeESPSection = ESPTab:CreateSection("üå≥ Tree ESP")

local treeHighlights = {}

local function updateTreeESP()
    -- Clear existing tree highlights
    for treeName, highlight in pairs(treeHighlights) do
        if highlight then
            highlight:Destroy()
        end
    end
    treeHighlights = {}
    
    if treeEspEnabled then
        local treesFolder = workspace:FindFirstChild("WorldSpawn")
        if treesFolder then
            treesFolder = treesFolder:FindFirstChild("Trees")
            if treesFolder then
                for _, tree in pairs(treesFolder:GetDescendants()) do
                    if tree:IsA("Part") or tree:IsA("MeshPart") then
                        local treeName = tree.Name
                        if selectedTrees[treeName] then
                            local highlight = Instance.new("Highlight")
                            highlight.Name = "TreeESP_" .. treeName
                            highlight.Adornee = tree
                            highlight.FillColor = Color3.fromRGB(139, 69, 19) -- Brown color for trees
                            highlight.OutlineColor = Color3.fromRGB(0, 255, 0)
                            highlight.FillTransparency = 0.3
                            highlight.OutlineTransparency = 0
                            highlight.Parent = tree
                            
                            treeHighlights[treeName] = highlight
                        end
                    end
                end
            end
        end
    end
end

local function toggleTreeESP()
    treeEspEnabled = not treeEspEnabled
    
    if treeEspEnabled then
        Rayfield:Notify({
            Title = "üå≥ Tree ESP",
            Content = "Tree ESP enabled",
            Duration = 3,
            Image = 0,
        })
        updateTreeESP()
        
        -- Listen for new trees
        local treesFolder = workspace:FindFirstChild("WorldSpawn")
        if treesFolder then
            treesFolder = treesFolder:FindFirstChild("Trees")
            if treesFolder then
                treesFolder.DescendantAdded:Connect(function(descendant)
                    if treeEspEnabled and (descendant:IsA("Part") or descendant:IsA("MeshPart")) then
                        if selectedTrees[descendant.Name] then
                            wait(0.1) -- Wait for tree to load
                            updateTreeESP()
                        end
                    end
                end)
            end
        end
    else
        Rayfield:Notify({
            Title = "üå≥ Tree ESP",
            Content = "Tree ESP disabled",
            Duration = 3,
            Image = 0,
        })
        for treeName, highlight in pairs(treeHighlights) do
            if highlight then
                highlight:Destroy()
            end
        end
        treeHighlights = {}
    end
end

-- Create tree selection
for _, treeName in pairs(allTrees) do
